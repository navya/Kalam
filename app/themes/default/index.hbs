<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{{number}}}:{{{title}}}</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto:regular,bold,italic,thin,light,bolditalic,black,medium&amp;lang=en" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="material.min.css">
    <link rel="stylesheet" href="styles.css">
    <style>
    </style>
  </head>
  <body>
    <div class="demo-layout mdl-layout mdl-js-layout mdl-layout--fixed-drawer mdl-layout--fixed-header">
      <header class="demo-header mdl-layout__header mdl-color--white mdl-color--grey-100 mdl-color-text--grey-600">
        <div class="mdl-layout__header-row">
          <span class="mdl-layout-title">{{{number}}}: {{{title}}} As Taught in {{{session}}}</span>
          <div class="mdl-layout-spacer"></div>
          <div class="mdl-textfield mdl-js-textfield mdl-textfield--expandable">
            By- {{{instructors}}}
          </div>
        </div>
      </header>
      <div class="demo-drawer mdl-layout__drawer mdl-color--blue-grey-900 mdl-color-text--blue-grey-50">
        <nav class="demo-navigation mdl-navigation mdl-color--blue-grey-800">
          <a class="mdl-navigation__link" href=""><i class="mdl-color-text--blue-grey-400 material-icons" role="presentation">playlist_add</i>Overview</a>
          {{#if schedule}}
          <a class="mdl-navigation__link" href="#schedule"><i class="mdl-color-text--blue-grey-400 material-icons" role="presentation">dashboard</i>Schedule</a>{{/if}}
          {{#if assignments}}
          <a class="mdl-navigation__link" href="#assignments"><i class="mdl-color-text--blue-grey-400 material-icons" role="presentation">assignment</i>Assignments</a>{{/if}}
          {{#if notes}}
          <a class="mdl-navigation__link" href="#notes"><i class="mdl-color-text--blue-grey-400 material-icons" role="presentation">assignment</i>Lecture Notes</a>{{/if}}
          {{#if announcements}}
          <a class="mdl-navigation__link" href="#announcements"><i class="mdl-color-text--blue-grey-400 material-icons" role="presentation">event</i>Announcements</a>{{/if}}
          {{#if grading_schema}}
          <a class="mdl-navigation__link" href="#grading_schema"><i class="mdl-color-text--blue-grey-400 material-icons" role="presentation">alarm_on</i>Grading Policy</a>{{/if}}
          {{#if references}}
          <a class="mdl-navigation__link" href="references"><i class="mdl-color-text--blue-grey-400 material-icons" role="presentation">alarm_on</i>References</a>{{/if}}
          {{#if contact}}
          <a class="mdl-navigation__link" href="#contact"><i class="mdl-color-text--blue-grey-400 material-icons" role="presentation">contacts</i>Contact Details</a>{{/if}}
          <div class="mdl-layout-spacer"></div>
        </nav>
      </div>
      <main class="mdl-layout__content section--center mdl-color--grey-100">
      <div class="mdl-grid ">
        {{#if contact}}
        <a name="contact"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            {{{contact}}}
          </div>
        </div>
        {{/if}}
        {{#if announcements}}
        <a name="announcements"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>Announcements</h4>
            {{{announcements}}}
          </div>
        </div>
        {{/if}}
        {{#if objective}}
        <a name="objective"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>Course Objective:</h4>
            {{{objective}}}
          </div>
        </div>
        {{/if}}
        {{#if description}}
        <a name="description"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>Course Description:</h4>
            {{{description}}}
          </div>
        </div>
        {{/if}}
        {{#if content}}
        <a name="content"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>Course Content:</h4>
            {{{content}}}
          </div>
        </div>
        {{/if}}
        {{#if projects}}
        <a name="projects"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>Course Projects:</h4>
            {{{projects}}}
          </div>
        </div>
        {{/if}}
        {{#if schedule}}
        <a name="schedule"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>Schedule</h4>
            {{{schedule}}}
          </div>
        </div>
        {{/if}}
        {{#if references}}
        <a name="references"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>References</h4>
            {{{references}}}
          </div>
        </div>
        {{/if}}
        {{#if grading_schema}}
        <a name="grading_schema"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>Grading Schema</h4>
            {{{grading_schema}}}
          </div>
        </div>
        {{/if}}
        {{#if assignments}}
        <a name="assignments"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>Assignments</h4>
            <ul >
              {{#assignments}}
              <li><a href="downloads/{{ename}}">{{'name'}}</a></li>
              {{/assignments}}
            </ul>
          </div>
        </div>
        {{/if}}
        {{#if notes}}
        <a name="notes"></a>
        <div class="mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
          <div class="mdl-card__supporting-text">
            <h4>Lecture Notes</h4>
            <ul >
              {{#notes}}
              <li><a href="downloads/{{ename}}">{{'name'}}</a></li>
              {{/notes}}
            </ul>
          </div>
        </div>
        {{/if}}
        </main>
      </div>
      
      <script src="material.min.js"></script>
    </body>
  </html>